int x = 0;
int han = 70;
long mato1;

void setup()
{
    pinMode(1, OUTPUT);
    pinMode(2, OUTPUT);
    pinMode(3, OUTPUT);
    pinMode(4, OUTPUT);
    pinMode(5, OUTPUT);
}

void loop()
{
    mato1 = random(3);
    delay(1500);
    if (mato1 = 0)
    {
        analogWrite(1, han);
    }else if (mato1 = 1)
    {
        analogWrite(2, han);
    }else if (mato1 = 2)
    {
        analogWrite(3, han);
    }else if (mato1 = 3)
    {
        analogWrite(4, han);
    }else if (mato1 = 4)
    {
        analogWrite(5, han);
    }
    
    }
}